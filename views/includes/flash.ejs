<% if(success && success.length) {%>
<div class="alert alert-success alert-dismissible fade show mt-2 mb-2 col-6 offset-3" role="alert">
   <%=success%>
   <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
   </button>
  </div>
<%}%>
<% if(error && error.length) {%>
  <div class="alert alert-danger alert-dismissible fade show mt-2 mb-2 col-5 offset-4"role="alert">
    <%=error%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
    </button>
   </div>
<%}%>


<% if(signup && signup.length) {%>
  <div class="alert alert-danger alert-dismissible fade show mt-2 mb-2" role="alert">
    <%=signup%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
    </button>
   </div>
<%}%>
